{
  "name": "@finos/perspective-viewer-datagrid",
  "version": "3.0.3",
  "description": "Perspective datagrid plugin based on `regular-table`",
  "unpkg": "dist/cdn/perspective-viewer-datagrid.js",
  "jsdelivr": "dist/cdn/perspective-viewer-datagrid.js",
  "exports": {
    ".": "./dist/esm/perspective-viewer-datagrid.js",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "type": "module",
  "files": [
    "dist/**/*",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/perspective"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "chroma-js": "^1.3.4",
    "regular-table": "=0.6.4",
    "@finos/perspective": "^3.0.3",
    "@finos/perspective-viewer": "^3.0.3"
  },
  "devDependencies": {
    "@prospective.co/procss": "^0.1.16",
    "@finos/perspective-esbuild-plugin": "^3.0.3",
    "@finos/perspective-test": "^3.0.3"
  },
  "scripts": {
    "build": "node build.js",
    "clean": "rimraf dist",
    "clean:screenshots": "rimraf \"test/screenshots/**/*.@(failed|diff).png\""
  }
}